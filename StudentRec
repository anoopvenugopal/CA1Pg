class StudentRec:
   def __init__(self,stId,stname,coursecode):
        self.stId=stId
        self.stname=stname
        self.coursecode=coursecode
    def addData(self):
        self.stId=int(input("Enter IDno:\t"))
        self.stname = input("Enter Name:\t")
        self.coursecode = int(input("Enter corsecode:\t"))
    def retrieve(self):
        print("ID\t:",self.stId)
        print("Name\t:", self.stname)
        print("CourseCode\t:", self.coursecode)
def main():
    s1=StudentRec()
    s1.addData()
    s1.retrieve()
if __name__=="__main__":
    main()

